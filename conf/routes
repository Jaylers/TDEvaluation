# An example controller showing a sample home page
GET        /                                                            controllers.HomeController.index

#TODO: Split controller
GET        /signin                                                      controllers.HomeController.signIn
GET        /management/user                                             controllers.HomeController.managementUserList
GET        /management/evaluation                                       controllers.HomeController.managementEvaluationList
GET        /management/evaluation/edit/:id                              controllers.HomeController.managementEvaluationEdit(id: Long)
GET        /management/evaluation/:id                                   controllers.HomeController.managementEvaluationView(id: Long)
GET        /management/evaluation/:evaluationFormId/user/:userId        controllers.HomeController.managementEvaluationResponse(evaluationFormId: Long,userId: Long)
# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                                                controllers.Assets.versioned(path="/public", file: Asset)
